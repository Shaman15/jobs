<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.testepaggi">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.view.activity.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.view.activity.TransacaoActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".ui.view.activity.PagamentosActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".ui.view.activity.NovaTransacaoActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".ui.view.activity.SobreActivity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>